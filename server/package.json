{
    "name": "Eventorove-server",
    "version": "1.0.0",
    "description": "Eventorove Backend Server",
    "private": true,
    "scripts": {
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/jest/bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "typeorm": "ts-node ./node_modules/typeorm/cli",
        "migration:generate": "npm run typeorm -- migration:generate -d src/db/data-source.ts",
        "migration:run": "npm run typeorm -- migration:run -d src/db/data-source.ts",
        "migration:revert": "npm run typeorm -- migration:revert -d src/db/data-source.ts"
    },
    "dependencies": {
        "@nestjs/common": "10.4.20",
        "@nestjs/config": "3.2.0",
        "@nestjs/core": "10.4.20",
        "@nestjs/jwt": "^11.0.0",
        "@nestjs/platform-express": "10.4.20",
        "@nestjs/typeorm": "10.0.2",
        "@types/bcrypt": "^6.0.0",
        "@types/express-session": "^1.18.2",
        "bcrypt": "^6.0.0",
        "class-transformer": "0.5.1",
        "class-validator": "0.14.1",
        "date-fns": "3.6.0",
        "date-fns-tz": "3.2.0",
        "drizzle-orm": "^0.29.5",
        "drizzle-zod": "^0.5.1",
        "express-session": "^1.18.2",
        "glob": "10.3.10",
        "lru-cache": "10.2.0",
        "pg": "8.11.3",
        "reflect-metadata": "^0.2.2",
        "rimraf": "5.0.5",
        "rxjs": "7.8.1",
        "typeorm": "0.3.20",
        "typeorm-naming-strategies": "4.1.0",
        "zod": "^3.25.76"
    },
    "devDependencies": {
        "@nestjs/cli": "10.3.2",
        "@nestjs/schematics": "10.1.1",
        "@nestjs/testing": "10.4.20",
        "@types/express": "4.17.21",
        "@types/jest": "29.5.12",
        "@types/node": "20.11.28",
        "@types/supertest": "6.0.2",
        "@typescript-eslint/eslint-plugin": "^8.8.0",
        "@typescript-eslint/parser": "^8.8.0",
        "eslint": "^9.36.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-prettier": "^5.2.0",
        "jest": "29.7.0",
        "prettier": "^3.4.2",
        "source-map-support": "0.5.21",
        "supertest": "7.1.3",
        "ts-jest": "29.1.2",
        "ts-loader": "9.5.1",
        "ts-node": "10.9.2",
        "tsconfig-paths": "4.2.0",
        "typescript": "5.4.2"
    },
    "overrides": {
        "acorn-import-assertions": "npm:acorn-import-attributes@^1.9.0"
    }
}